<div class="snf-form text-align-left">
  <%= simple_form_for(@feedback) do |f| %>
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :domain %>
      <%= f.input :email %>
      <%= f.input :mobile_number %>
      <%= f.input :comment %>
      <%= f.button :submit, 'SUBMIT' %>
    </div>
  <% end %>
</div>
