<div class="snf-container" align="center">
  <div class="snf-heading text-align-left margin-bottom">
    <h2>Gstin</h2>
    <div class="margin-bottom">
      <label class="label-left">* Fields are mandatory</label>
    </div>
  </div>
  <%= render 'form', gstin: @gstin, action: "SHOW GSTIN" do |f| %>
    <div>
      <%= link_to 'Edit', edit_dashboard_gstin_path(@gstin), class: "btn btn-default" %>
      <%= link_to 'Back', dashboard_gstins_path, class: "btn btn-default" %>
    </div>
  <% end %>
</div>
