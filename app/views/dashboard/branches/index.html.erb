<h3>Branches</h3>
<div class="table-responsive">
  <table width="100%" class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Address</th>
        <th>Location</th>
        <th>Gstin</th>
        <th colspan="3">Action</th>
      </tr>
    </thead>
    <tbody>
      <% @branches.each do |branch| %>
          <tr>
            <td><%= branch.name %></td>
            <td><%= branch.address %></td>
            <td><%= branch.location %></td>
            <td><%= branch.gstin.gstin_number %></td>
            <td><%= link_to 'Show', dashboard_branch_path(branch) %></td>
            <td><%= link_to 'Edit', edit_dashboard_branch_path(branch) %></td>
            <td><%= link_to 'Destroy', dashboard_branch_path(branch), method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
